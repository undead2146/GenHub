{
  "GenHub": {
    "Workspace": {
      "DefaultPath": "",
      "DefaultStrategy": "HybridCopySymlink"
    },
    "Cache": {
      "DefaultPath": ""
    },
    "Downloads": {
      "DefaultTimeoutSeconds": 600,
      "DefaultUserAgent": "GenHub/1.0",
      "DefaultMaxConcurrent": 3,
      "DefaultBufferSize": 81920,
      "Policy": {
        "MinConcurrent": 1,
        "MaxConcurrent": 10,
        "MinTimeoutSeconds": 10,
        "MaxTimeoutSeconds": 3600,
        "MinBufferSizeBytes": 4096,
        "MaxBufferSizeBytes": 1048576
      }
    },
    "UI": {
      "DefaultTheme": "Dark",
      "DefaultWindowWidth": 1024,
      "DefaultWindowHeight": 768
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information"
    }
  }
}
